{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
      "airbnb-base",
      "prettier"
    ],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "quotes": [
        "error",
        "single"
    ]
  }
}
